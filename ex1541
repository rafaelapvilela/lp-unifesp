#include <stdio.h>
#include <math.h>

int main() {
  int a, b, c, area, p, result;
  while(1) {
    scanf("%d", &a);
    if(a==0) {
      break;
    } else {
      scanf("%d %d", &b, &c);
      area=a*b;
      p=(area*100)/c;
      result=pow(p, 0.5);
      printf("%d\n",result);
    }
  }
  return 0;
}
