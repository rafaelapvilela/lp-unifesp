#include <stdio.h>

int main(void) {
  int x, soma=0, i;
  scanf("%d", &x);
  while (x!=0) {
    i=5;
    soma=0;
    while(i>0){
      if (x%2==0) {
        soma = soma + x;
        i--;
        x = x + 2;
      } else {
        x = x + 1;
      }
    }
    printf("%d\n",soma);
    scanf("%d", &x);
  }
  return 0;
}
