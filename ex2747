#include <stdio.h>

int main(void) {
  int i, j;

  for (j=0;j<7;j++) {
    if(j==0||j==6) {
      for (i=0;i<39;i++) {
        printf("-");
        if (i==38) printf("\n");
      }
    } else {
      for(i=0;i<39;i++) {
        if(i==0) {
          printf("|");
        } else if (i==38) {
          printf("|\n");
        } else {
          printf(" ");
        }
      }
    }
  }
  return 0;
}
